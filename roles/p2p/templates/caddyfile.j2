"www.{{ domain }}", "{{ domain }}" {
				root	/var/www/html

				fastcgi /RPC2		flood:5000	php
				basicauth /RPC2		hanks		hanks
				tls {{ user_mail }}
}

"plex.{{ domain }}" {
				proxy 		/					plex:32400		{
				transparent
				}
				tls {{ user_mail }}
				timeouts none
}

